@model CruisinV2.Models.Maintenance

@{
    ViewData["Title"] = "Delete";
}

<h3>Are you sure you want to delete this?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Question
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MaintenanceQuestion)
        </dd>
        <dt class = "col-sm-2">
            Answer
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MaintenanceAnswer)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
    <a asp-action="Index">Back to List</a>
</div>
